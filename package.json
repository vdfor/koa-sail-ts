    "@types/mocha": "^5.2.0",
    "@types/node": "^12.12.14",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.4",
    "@types/ws": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "cross-env": "^5.1.4",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "fs-extra": "^8.1.0",
    "mocha": "^5.1.1",
    "nodemon": "^2.0.1",
